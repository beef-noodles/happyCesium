{
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {

  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": [1, 2],
    "linebreak-style": [2, "unix"],
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "semi-spacing": 0,
    "no-extra-semi": 1,
    "no-unused-vars": [1, {
      "vars": "all",
      "args": "none"
    }],
    "no-trailing-spaces": 2,
    "no-console": 0
  }
}
